    "keywords": [
        "silverstripe",
        "site config",
        "siteconfig",
        "modeladmin",
        "model admin",
        "configuration",
        "config"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Jono Menz",
            "homepage": "https://jonomenz.com"
        }
    ],
    "require": {
        "silverstripe/framework": "^4 | ^5"
    },
    "autoload": {
        "psr-4": {
            "JonoM\\SomeConfig\\": "src/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
